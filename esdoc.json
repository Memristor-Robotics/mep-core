{
  "source": "./src",
  "destination": "./docs/api",
  "includes": [ "\\.js$" ],
  "excludes": [ "\\.experiment.js$" ],

  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "brand": {
          "title": "MEP Master"
        },
        "manual": {
          "index": "README.md",
          "files": [
            "./docs/Introduction.md",
            "./docs/Source_Roadmap.md",
            "./docs/Log.md",
            "./docs/Goals.md",
            "./docs/Inspiration.md",
            "./docs/Creating_Driver.md"
          ]
        },
        "test": {
          "type": "mocha",
          "source": "test"
        }
      }
    }
  ]
}
